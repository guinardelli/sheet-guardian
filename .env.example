# Supabase Configuration
# Get these values from your Supabase project settings: https://app.supabase.com/project/_/settings/api

# Your Supabase project ID (found in project URL)
VITE_SUPABASE_PROJECT_ID="your_project_id_here"

# Your Supabase anon/public key (safe to expose in frontend)
VITE_SUPABASE_PUBLISHABLE_KEY="your_publishable_anon_key_here"

# Your Supabase project URL
VITE_SUPABASE_URL="https://your-project-id.supabase.co"

# Stripe configuration (used by Supabase Edge Functions)
STRIPE_SECRET_KEY="sk_live_or_test_key_here"
STRIPE_WEBHOOK_SECRET="whsec_webhook_secret_here"
STRIPE_ANNUAL_PRICE_ID="price_xxxxx"

# Stripe public IDs (used by frontend)
VITE_STRIPE_PROFESSIONAL_PRODUCT_ID="prod_xxxxx"
VITE_STRIPE_PROFESSIONAL_PRICE_ID="price_xxxxx"
VITE_STRIPE_PREMIUM_PRODUCT_ID="prod_xxxxx"
VITE_STRIPE_PREMIUM_PRICE_ID="price_xxxxx"
VITE_STRIPE_ANNUAL_PRICE_ID="price_xxxxx"

# Supabase service role key (required for server-side operations like webhooks)
SERVICE_ROLE_KEY="your_service_role_key_here"

# Admin secret for protected maintenance endpoints (cleanup-tokens, health-check)
ADMIN_SECRET="your_admin_secret_here"

# Optional: Resend email provider (upgrade confirmation)
# RESEND_API_KEY="re_XXXXXXXXXXXXXXXXXXXXXXXX"
# RESEND_FROM_EMAIL="Sheet Guardian <no-reply@yourdomain.com>"

# Optional: Sentry DSN for error tracking (add when implementing monitoring)
# VITE_SENTRY_DSN="your_sentry_dsn_here"

# Optional: Log level (debug/info/warn/error)
# VITE_LOG_LEVEL="debug"

# Optional: Feature flags
# VITE_FEATURE_2FA="false"
# VITE_FEATURE_BATCH="false"

# Optional: Analytics (add when implementing)
# VITE_PLAUSIBLE_DOMAIN="yourdomain.com"
